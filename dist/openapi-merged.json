                    "id": {
                      "type": "integer",
                      "description": "id",
                      "example": 1
                    },
                    "email": {
                      "type": "string",
                      "description": "メールアドレス",
                      "maxLength": 255,
                      "example": "ishikawa@sg.sandbox"
                    },
                    "emailVerifiedAt": {
                      "type": "string",
                      "nullable": true,
