    "/my/user": {
      "get": {
        "tags": [
          "my/user"
        ],
        "summary": "ユーザー情報取得",
        "description": "ログインユーザのユーザー情報を取得",
        "responses": {
          "200": {
            "description": "正常応答",
            "content": {
              "application/json": {
                "schema": {
                  "title": "User",
                  "type": "object",
